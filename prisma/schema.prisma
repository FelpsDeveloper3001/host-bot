generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./host.db"
}

model Interactions {
  id        String  @id @default(uuid())
  customId  String  @unique
  channelId String?
  createdBy String?
  extra     String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("interactions")
}
